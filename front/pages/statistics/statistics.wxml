<!--pages/statistics/statistics.wxml-->
<van-notify id="van-notify" />

<view
    class="{{isAdmin?'hidden':'not-hidden'}}"
    wx:for="{{records}}"
    wx:key="{{item.time}}"
>
    <van-cell
        title="{{item.course.courseName}}"
        value="{{item.course.teacher}}"
        label="{{item.time}}"
    />
</view>